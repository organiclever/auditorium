{
  "root": "libs/ts-utils",
  "targets": {
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "libs/ts-utils",
        "commands": ["yarn test"]
      }
    },
    "test-watch": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "libs/ts-utils",
        "commands": ["yarn test:watch"]
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "libs/ts-utils",
        "commands": ["yarn build"]
      }
    }
  },
  "implicitDependencies": []
}
